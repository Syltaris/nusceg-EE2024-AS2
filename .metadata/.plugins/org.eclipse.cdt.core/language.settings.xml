<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<plugin>
	<extension point="org.eclipse.cdt.core.LanguageSettingsProvider">
		<provider class="org.eclipse.cdt.managedbuilder.language.settings.providers.GCCBuildCommandParser" id="com.crt.advproject.GCCBuildCommandParser" keep-relative-paths="false" name="MCU GCC Build Output Parser" parameter="(arm-none-eabi-gcc)|(arm-none-eabi-[gc]\+\+)|(gcc)|([gc]\+\+)|(clang)" prefer-non-shared="false">
			<language id="org.eclipse.cdt.core.gcc">
				<resource project-relative-path="src/main.c">
					<entry kind="includePath" name="/Lib_CMSISv1p30_LPC17xx/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="includePath" name="/Lib_EaBaseBoard/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="includePath" name="/Lib_MCU/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="macro" name="DEBUG" value=""/>
					<entry kind="macro" name="__CODE_RED" value=""/>
					<entry kind="macro" name="__NEWLIB__" value=""/>
					<entry kind="macro" name="__USE_CMSIS" value="CMSISv1p30_LPC17xx"/>
				</resource>
				<resource project-relative-path="src/cr_startup_lpc176x.c">
					<entry kind="macro" name="DEBUG" value=""/>
					<entry kind="macro" name="__REDLIB__" value=""/>
				</resource>
				<resource project-relative-path="src/cr_startup_lpc17.c">
					<entry kind="includePath" name="/Lib_CMSISv1p30_LPC17xx/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="includePath" name="/Lib_EaBaseBoard/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="includePath" name="/Lib_MCU/inc">
						<flag value="VALUE_WORKSPACE_PATH|RESOLVED"/>
					</entry>
					<entry kind="macro" name="DEBUG" value=""/>
					<entry kind="macro" name="__CODE_RED" value=""/>
					<entry kind="macro" name="__NEWLIB__" value=""/>
					<entry kind="macro" name="__USE_CMSIS" value="CMSISv1p30_LPC17xx"/>
				</resource>
			</language>
		</provider>
	</extension>
</plugin>
